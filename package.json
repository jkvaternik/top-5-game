{
  "name": "top5-game",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "validate": "node app/data/validatePuzzles.ts",
    "prepush": "yarn run lint && yarn run validate",
    "dev": "next dev",
    "build": "npm run build:app && npm run build:functions",
    "build:app": "next build",
    "build:functions": "netlify-lambda build netlify/functions --config ./webpack.functions.js",
    "start": "next start",
    "server": "node server.js",
    "netlify-build": "next build  && npm run build:functions"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "downshift": "^8.3.2",
    "express": "^4.19.2",
    "fuse.js": "^7.0.0",
    "moment": "^2.30.1",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.4",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.11",
    "netlify-lambda": "^2.0.16",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}